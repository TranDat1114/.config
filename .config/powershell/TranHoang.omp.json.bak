{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#2ccfcf",
          "properties": {
            "template": "Tr\u1ea7n Ho√†ng <#32CD30>\ue22f</>\u276f "
          },
          "style": "plain",
          "type": "text"
        },
        {
          "background": "#FBC740",
          "foreground": "#000",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "folder",
            "template": " {{ .Path }} "
          },
          "style": "powerline",
          "type": "path"
        },
        {
          "background": "#BD97CB",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true,
            "template": " \u279c ({{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}) "
          },
          "style": "powerline",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        
        {
          "background": "#F9E950",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#FBC740{{end}}",
            "{{if eq \"Discharging\" .State.String}}#E56997{{end}}",
            "{{if eq \"Full\" .State.String}}#F9E950{{end}}"
          ],
          "foreground": "#262626",
          "invert_powerline": true,
          "properties": {
            "charged_icon": "\uf004 ",
            "charging_icon": "\uf21e ",
            "discharging_icon": "\uf08a ",
            "template": "\ue0b0 {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 \ue0d4"
          },
          "style": "diamond",
          "type": "battery"
        },
        {
          "background": "#66D2D6",
          "foreground": "#262626",
          "properties": {
            "style": "roundrock",
            "template": "\ue0d2 \ufa1e{{ .FormattedMs }}\u2800 \ue0c5",
            "threshold": 0
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#21c7c7",
          "properties": {
            "template": "\u2570\u2500"
          },
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "#e0f8ff",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "\ue285\ufb00{{ if gt .Code 0 }}\uf00d{{ else }}\uf42e{{ end }} "
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {}
  ],
  "final_space": true,
  "version": 1
}
