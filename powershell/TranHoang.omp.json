{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "final_space": true,
    "blocks": [
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
          {
            "type": "session",
            "style": "diamond",
            "foreground": "#ffffff",
            "background": "#21C5C4",
	    "leading_diamond": "╭─\uE0B6",
            "properties": {
              "prefix": " <#FD7F20>\uf829</> ",
              "template": "{{ .UserName }}"
            }
          },
          {
            "type": "path",
            "style": "powerline",
            "powerline_symbol": "\uE0B0",
            "foreground": "#ffffff",
            "background": "#F9E950",
            "properties": {
              "style": "folder"
            }
          },
          {
            "type": "git",
            "style": "powerline",
            "powerline_symbol": "\uE0B0",
            "foreground": "#ffffff",
            "background": "#BD97CB",
            "properties": {
              "fetch_stash_count": true,
              "fetch_upstream_icon": true,
              "branch_icon": "",
              "fetch_status": false,
              "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }}",
              "prefix": " \u279C (",
              "postfix": ") "
            }
          },
          {
            "type": "time",
            "style": "diamond",
            "trailing_diamond": "\uE0B0",
            "foreground": "#ffffff",
            "background": "FF94B2",
            "properties": {
              "prefix": " \ue255 ",
              "time_format": "15:04"
            }
          }
        ]
      },
          {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
	  "type": "text",
	  "style": "plain",
	  "foreground": "#C14F60",
  	  "properties": {
  	      "prefix": "Trần Hoàng <#32CD30>\ue22f</>",
   	      "template": " \u276F"
	  }
	},
	{
          "type": "node",
          "style": "diamond",
          "leading_diamond": " \uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#3C873A",
          "background": "#BD97CB",
          "properties": {
            "prefix": "\uE718 ",
            "postfix": " \ue0d4",
            "display_package_manager": true,
            "yarn_icon": " <#348cba></>",
            "npm_icon": " <#cc3a3a></> "
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "leading_diamond": " \uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#FFE873",
          "background": "#BD97CB",
          "properties": {
            "prefix": " \uf818",
            "postfix": " "
          }
        },
        {
          "type": "java",
          "style": "diamond",
          "leading_diamond": " \uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#ec2729",
          "background": "#BD97CB",
          "properties": {
            "prefix": " \ue256 ",
            "postfix": " "
          }
        },
        {
          "type": "dotnet",
          "style": "diamond",
          "leading_diamond": " \uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#0d6da8",
          "background": "#BD97CB",
          "properties": {
            "prefix": " \uf81a ",
            "postfix": " "
          }
        },
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "",
          "foreground": "#222222",
          "background": "#FF94B2",
          "properties": {
            "postfix": "  \ue0d4",
            "windows": "\ue27f"
          }
        },
        {
          "type": "battery",
          "style": "diamond",
          "invert_powerline": true,
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "#262626",
          "background": "#F9E950",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#FBC740{{end}}",
            "{{if eq \"Discharging\" .State.String}}#E56997{{end}}",
            "{{if eq \"Full\" .State.String}}#F9E950{{end}}"
          ],
          "properties": {
            "discharging_icon": "\uf08a ",
            "charging_icon": "\uf21e ",
            "charged_icon": "\uf004 ",
	    "prefix": "\ue0d2 ",
            "postfix": " \ue0d4"
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "\uE0B0",
          "foreground": "#262626",
          "background": "#66D2D6",
          "properties": {
            "prefix": "\ue0d2 羽",
            "postfix": "\u2800 \ue0c5",
            "threshold": 0,                  
            "style": "roundrock"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#21c7c7",
          "properties": {
            "prefix": "",
            "postfix": "",
            "text": "╰─"
          }
        },
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#e0f8ff",
          "properties": {
            "prefix": "\ue285ﬀ",
            "display_exit_code": false,
            "always_enabled": true,
            "error_color": "#ef5350"
          }
        }
      ]
    },
    {
        "transient_prompt": {
        "background": "transparent",
        "foreground": "#ffffff",
        "template": "{{ .Shell }}> "
        }
     }
  ]
}
